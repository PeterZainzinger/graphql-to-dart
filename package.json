{
  "name": "graphql-to-dart",
  "version": "0.0.4-alpha3",
  "description": "generate dart json_serializable classes using graphql-code-generator",
  "dependencies": {
    "linewrap": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micimize/graphql-to-jaguar-dart.git"
  },
  "license": "MIT",
  "author": "Michael Joseph Rosenthal",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "codegen-handlebars-templates-scripts build",
    "pretest": "yarn build",
    "test": "codegen-handlebars-templates-scripts test"
  },
  "devDependencies": {
    "codegen-handlebars-templates-scripts": "0.9.2",
    "graphql-codegen-core": "0.9.2",
    "graphql-codegen-compiler": "0.9.2",
    "graphql": "0.13.2",
    "@types/graphql": "0.13.0"
  },
  "main": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "dist"
  ]
}
