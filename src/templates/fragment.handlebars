{{> defineClass
      fields=(hackFragmentFields "get" fragmentsSpread fields)
      className=(concat (toPascalCase name) '')
      contextModels=innerModels
      contextName=(toPascalCase name)
}}

{{ hackFragmentFields "add" name fields innerModels }}

