class {{ className }} {{ classExtends interfaces }} {
  {{#each this}}{{#if type}}{{#if name}}
  {{#if description }}{{ multilineComment description }}
  {{/if}}final {{ toPrimitive type}} {{name}};
  {{~/if}}{{/if}}{{/each}}

  {{className}}({
    {{#each this}}{{#if name}}
    {{~#if isRequired}}@required {{/if}}this.{{name}},
    {{/if}}{{/each}}
  });
}
