import 'package:meta/meta.dart';
import '{{ config.customScalars }}';

/* Enums */
{{#each enums}}
{{> enum }}

{{/each}}
{{#each unions}}
{{ toComment description }}
export type {{ name }} = {{#each possibleTypes}}{{this}}{{#unless @last}} | {{/unless}}{{/each}};

{{/each}}

/* Input Types */
{{#each inputTypes}}
{{> type }}

{{/each}}

/* Interfaces */
{{#each interfaces}}
{{> type }}

{{/each}}

/* Types */
{{#each types}}
{{> type }}

{{/each}}
{{!--
{{~#each types}}
  {{~#each fields}}
    {{~# if hasArguments }}
export interface {{ toPascalCase name }}{{ toPascalCase ../name }}Args {
{{#each arguments}}
  {{ name }}{{ getOptionals this }}: {{ convertedType this }}; {{ toComment description }}
{{/each}}
}
    {{/if}}
  {{/each}}
{{/each}}
--}}

